@model EntityLayer.Tag;
@{
    ViewData["Title"] = "Guncelle";
    Layout = "~/Views/Shared/_Admin.cshtml";
}

<h1>ETİKET GÜNCELLE</h1>
<form asp-action="Guncelle" asp-controller="Tag" method="post">
    <div class="mb-3">
        <label class="form-label">ETİKET İÇERİĞİ</label>
        <input type="text" class="form-control" asp-for="tag_Name">
    </div>

    <button type="submit" class="btn btn-primary">ETİKET GÜNCELLE </button>
</form>


