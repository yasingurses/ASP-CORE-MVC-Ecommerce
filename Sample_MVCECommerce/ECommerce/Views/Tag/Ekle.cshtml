@model EntityLayer.Tag
@{
    ViewData["Title"] = "Ekle";
    Layout = "~/Views/Shared/_Admin.cshtml";
}

<h1>ETİKET EKLE</h1>
<form asp-action="Ekle" asp-controller="Tag" method="post">
    <div class="mb-3">
        <label class="form-label">ETİKET İÇERİĞİ</label>
        <input type="text" class="form-control" asp-for="tag_Name">
    </div>

    <button type="submit" class="btn btn-primary">ETİKET EKLE</button>
</form>

